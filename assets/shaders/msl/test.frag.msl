#include <metal_stdlib>
#include <simd/simd.h>

using namespace metal;

struct type_Test
{
    float4 test;
};

struct main0_out
{
    float4 out_var_SV_Target0 [[color(0)]];
};

struct main0_in
{
    float3 in_var_TEXCOORD2 [[user(locn2)]];
};

fragment main0_out main0(main0_in in [[stage_in]], constant type_Test& Test [[buffer(0)]])
{
    main0_out out = {};
    out.out_var_SV_Target0 = float4(float3(0.5) + (cos((float3(Test.test.x) + fast::normalize(in.in_var_TEXCOORD2)) + float3(0.0, 2.0, 4.0)) * 0.5), 1.0);
    return out;
}

